  <div class="main-content container">
        <div class="row">
          <div class="col-sm-12">
            <div class="panel panel-default panel-table">
              <div class="panel-heading"><span class="box-title">Listagem de Contas Bancárias</span>
                <div class="pull-right">
                 
                  <div class="tools pull-right" style="margin-top: -8px"><%= link_to new_account_bank_path do %><button class="btn btn-rounded btn-primary">Nova Conta</button><% end %></div>
               </div>
              </div>
              <div class="panel-body">
              
                  <table id="table_inter" class="table table-striped table-bordered dt-responsive nowrap display"  cellspacing="0" width="100%">
                  <thead>
                    <tr>
                      <th>Agência</th>
                      <th>Número da conta</th>
                      <th>Limite</th>
                      <th>Saldo</th>
                      <th class="noExport"></th>
                      <th class="noExport"></th>
                      <th class="noExport"></th>
                    </tr>
                  </thead>
                  <tbody>
                    <% @account_banks.each do |account_bank| %>
                    <tr class="odd gradeX">

                      <td><%= account_bank.agency_bank.number_agency %></td>
                      <td><%= account_bank.number_account %></td>
                      <td><%= number_to_currency(account_bank.limit) %></td>
                      <td><%= number_to_currency(account_bank.saldo) %></td>

                      <td class="actions"><%= link_to account_bank_path(account_bank), class: 'icon' do %><i class="s7-look"></i> Ver Movimentações<% end %></td>

                      <td class="actions"><%= link_to edit_account_bank_path(account_bank), class: 'icon' do %><i class="s7-settings"></i> Editar<% end %></td>

                      <td class="actions"><%= link_to account_bank, method: :delete, data: { confirm: 'Deseja realmente apagar a conta?' }, class: 'icon' do %><i class="s7-trash"></i>Apagar<% end %> 

                      </td>
                    </tr>
                    <% end %>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>