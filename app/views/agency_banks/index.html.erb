  <div class="main-content container">
        <div class="row">
          <div class="col-sm-12">
            <div class="panel panel-default panel-table">
              <div class="panel-heading"><span class="box-title">Listagem de agências</span>
                <div class="pull-right">
                 
                  <div class="tools pull-right" style="margin-top: -8px"><%= link_to new_agency_bank_path do %><button class="btn btn-rounded btn-primary">Nova agência</button><% end %></div>
               </div>
              </div>
              <div class="panel-body">
              
                  <table id="table_inter" class="table table-striped table-bordered dt-responsive nowrap display"  cellspacing="0" width="100%">
                  <thead>
                    <tr>
                      <th>Número da agência</th>
                      <th>Endereço</th>
                      <th></th>
                      <th></th>
                      <th></th>
                    </tr>
                  </thead>
                  <tbody>
                    <% @agency_banks.each do |agency_bank| %>
                    <tr class="odd gradeX">

                      <td><%= agency_bank.number_agency %></td>
                      <td><%= agency_bank.address %></td>
                      <td class="actions"><%= link_to agency_bank_path(agency_bank), class: 'icon' do %><i class="s7-look"></i> Ver Contas<% end %></td>

                      <td class="actions"><%= link_to edit_agency_bank_path(agency_bank), class: 'icon' do %><i class="s7-settings"></i> Editar<% end %></td>

                      <td class="actions"><%= link_to agency_bank, method: :delete, data: { confirm: 'Deseja realmente apagar a agência?' }, class: 'icon' do %><i class="s7-trash"></i>Apagar<% end %> 

                      </td>
                    </tr>
                    <% end %>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>