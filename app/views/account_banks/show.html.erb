
  <div class="main-content container">
        <div class="row">
          <div class="col-sm-12">
            <div class="panel panel-default panel-table">
              <div class="panel-heading"><span class="box-title">Movimentações - Conta <%= @account_bank.number_account %> - Agência <%= @account_bank.agency_bank.number_agency %></span>
                <div class="pull-right">
                 
                  <div class="tools pull-right" style="margin-top: -8px"><%= link_to account_banks_path do %><button class="btn btn-rounded btn-primary">Voltar</button><% end %></div>
               </div>
              </div>
              <div class="panel-body">
              
                  <table id="table_inter" class="table table-striped table-bordered dt-responsive nowrap display"  cellspacing="0" width="100%">
                  <thead>
                    <tr>
                      <th>Data</th>
                      <th>Tipo</th>
                      <th>Valor</th>
                      <th>Usuário</th>
                      <th></th>
                    </tr>
                  </thead>
                  <tbody>
                    <% @transactions.each do |transaction| %>
                    <tr class="odd gradeX">
                      <td><%= transaction.created_at %></td>
                      <td><%= transaction.transaction_type %></td>
                      
                      <td><%= transaction.price %></td>
                      <td><%= transaction.user.email %></td>

                      <td class="actions"><%#= link_to transaction, method: :delete, data: { confirm: 'Deseja realmente apagar a conta?' }, class: 'icon' do %><i class="s7-trash"></i>Estornar<%# end %> 

                      </td>
                    </tr>
                    <% end %>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>